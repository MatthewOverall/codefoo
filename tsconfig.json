{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "allowJs": true,
    "noEmit": true,
    "preserveConstEnums": true,
    "declaration": false
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}
